{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "SecretKey": "your-very-secure-secret-key-here", // Use a strong secret
    "Issuer": "user-service-issuer", // The issuer of the JWT (typically your auth server)
    "Audience": "user-service-api" // The expected audience for the token
  },
  "AllowedHosts": "*"
}
